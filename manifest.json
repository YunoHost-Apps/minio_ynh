{
    "name": "MinIO server",
    "id": "minio",
    "packaging_format": 1,
    "description": {
        "en": "High Performance, Kubernetes Native Object Storage",
        "fr": "Serveur de stockage d'objets hautes performances"
    },
    "version": "2023.02.27~ynh1",
    "url": "https://min.io/",
    "upstream": {
        "license": "AGPL-3.0-only",
        "website": "https://min.io",
        "admindoc": "https://docs.min.io/",
        "code": "https://github.com/minio/minio",
        "cpe": "cpe:2.3:a:minio:minio"
    },
    "license": "AGPL-3.0-only",
    "maintainer": {
        "name": "Raoul de Limezy"
    },
    "requirements": {
        "yunohost": ">= 11.0.9"
    },
    "multi_instance": false,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install": [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            },
            {
                "name": "admin",
                "type": "user"
            },
            {
                "name": "password",
                "type": "password"
            }
        ]
    }
}
