{
    "name": "MinIO server",
    "id": "minio",
    "packaging_format": 1,
    "description": {
        "en": "High Performance, Kubernetes Native Object Storage",
        "fr": "Serveur de stockage d'objets hautes performances"
    },
    "version": "2022.01.28~ynh1",
    "url": "https://min.io/",
    "upstream": {
        "license": "AGPL-3.0-only",
        "website": "https://min.io",
        "admindoc": "https://docs.min.io/",
        "code": "https://github.com/minio/minio"
    },
    "license": "AGPL-3.0-only",
    "maintainer": {
        "name": "Raoul de Limezy"
    },
    "requirements": {
        "yunohost": ">= 4.3.0"
    },
    "multi_instance": false,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain",
                "example": "minio.domain.tld",
                "ask": "Enter a domain for MinIO server. This has to be a fully dedicated domain"
            },
            {
                "name": "console_domain",
                "type": "domain",
                "example": "console.minio.domain.tld",
                "ask": "Enter a domain for MinIO console. This has to be a fully dedicated domain",
                "optional": true
            },
        ]
    }
}
